    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/express": "^4.16.1",
        "@types/http-status": "^0.2.30",
        "@types/jest": "^24.0.9",
        "@types/node": "^11.9.6",
        "jest": "^24.1.0",
        "rimraf": "^2.6.3",
